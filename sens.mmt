[[script]]
import myokit
import numpy as np
import matplotlib.pyplot as plt

m, p, _ = myokit.load('example')

# Run a simulation
s = myokit.PSimulation(m, p, variables=['membrane.V'], parameters=['ina.gNa', 'ica.gCa'])
d, e = s.run(1000)

# Plot the results
plt.figure()
plt.subplot(3, 1, 1)
plt.plot(d.time(), d['membrane.V'])

plt.subplot(3, 1, 2)
plt.plot(d.time(), e[:, 0, 0])

plt.subplot(3, 1, 3)
plt.plot(d.time(), e[:, 0, 1])
plt.show()

