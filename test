#!/usr/bin/env python3
import myokit
import numpy as np

n = 1000
m = 100
r = 80

b = myokit.Benchmarker()
x = []
for i in range(n):
    y = [0]*m
    for j in range(m):
        z = [0]*r
        y[j] = z
    x.append(y)
x = np.array(x, copy=False)
print(b.time())
print(x.shape)
del(b, x, y, z)

b = myokit.Benchmarker()
x = []
for i in range(n):
    y = [0]*m
    for j in range(m):
        z = (0)*r
        y[j] = z
    x.append(y)
x = np.array(x, copy=False)
print(b.time())
print(x.shape)
del(b, x, y, z)

b = myokit.Benchmarker()
x = []
for i in range(n):
    for j in range(m):
        for k in range(r):
            x.append(0)
x = np.array(x, copy=False)
x.reshape((n, m, r))
print(b.time())
print(x.shape)
